#include <stdio.h>
#define size 5
	int arr[size];
	int top=-1;
	int isstackfull() {
		if(top==size-1)
			return 1;
			return 0;
	}
	void push(int val) {
		if(isstackfull())
		printf("unable to push %d as the stact is Full\n",val);
		else
		{
			top++;
			arr[top]=val;
		}
	}
	int isstackempty() {
		if(top==-1) 
			return 1;
			return 0;
		}
void pop()
  {
      if(isstackempty())
          printf("Stack Is Empty\n");
      else
      {
          printf("Popped element = %d\n",arr[top]);
          top--;
      }

  }
	int main () {
		push(10);
		push(12);
		push(13);
		push(14);
		push(15);
		push(16);
		pop();
		pop();
		pop();
		pop();
		pop();
		pop();
		returnÂ 0;
	}
